FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1000000;

boundaryField
{
    inlet
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 1000000;
    }
    oulet
    {
        type            fixedValue;
        value           uniform 1000000;
    }
    fixedWalls
    {
        type            empty;
    }
    fluid_to_kernel
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 1000000;
    }
}


