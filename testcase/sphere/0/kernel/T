FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/kernel1";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 300;

boundaryField
{
    kernel_to_fluid
    {
        type            massTransferCoupled;
        kappaMethod     solidThermo;
        kappa           none;
        value           $internalField;
    }

}

