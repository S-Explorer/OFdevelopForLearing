FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       dictionary;
    location    "system/kernel1";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    h
    {
        solver           PCG;
        preconditioner   DIC;
        tolerance        1e-06;
        relTol           0.1;
    }
    hFinal
    {
        $h;
        tolerance        1e-7;
        relTol           0;
    }
}

PIMPLE
{
    momentumPredictor       no;
    nOuterCorrectors        5;
    nCorrectors             1;
    nNonOrthogonalCorrectors    0;
    outerCorrectorResidualControl
    {
      h
      {
          tolerance         1e-5;
          relTol            0.0;
      }
    }
}

