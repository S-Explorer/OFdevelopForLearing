FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//#include "solverControls"

libs            (MassTransferCoupled);

application     kernelPhaseTransition4FOAM;

startFrom       startTime;

startTime       400;

stopAt          endTime;

endTime         4800;

deltaT          1e-3;

writeControl    adjustable;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

maxCo           1;

maxDi           10.0;

adjustTimeStep  yes;

// ************************************************************************* //
functions
{
  solverInfo
  {
    type            solverInfo;
    libs            ("libutilityFunctionObjects.so");
    enabled         true;
    writeControl    none;
    region          fluid;
    fields          (p_rgh);
    writeResidualFields yes;
  }
}
