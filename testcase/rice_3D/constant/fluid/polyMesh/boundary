4
(
    inlet
    {
        type            patch;
        nFaces          100;
        startFace       192144;
    }
    oulet
    {
        type            patch;
        nFaces          100;
        startFace       192244;
    }
    fixedWalls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          800;
        startFace       192344;
    }
    fluid_to_hull
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          7568;
        startFace       193144;
        sampleMode      nearestPatchFace;
        sampleRegion    hull;
        samplePatch     hull_to_fluid;
    }
